#, kde-format
msgctxt "Name of the fallback \"freedesktop\" sound theme"
msgid "FreeDesktop"
msgstr "FreeDesktop"

#: kcm_soundtheme.cpp:112
#, kde-format
msgid "Fallback sound theme from freedesktop.org"
msgstr "Альтэрнатыўная гукавая тэма з freedesktop.org"

#: ui/main.qml:29
#, kde-format
msgctxt "@option:check"
msgid "Enable notification sounds"
msgstr "Уключыць гукі для апавяшчэнняў"

#: ui/main.qml:96
#, kde-format
msgctxt ""
"%2 is a theme name or a list of theme names that the current theme inherits "
"from"
msgid "Based on: %2"
msgid_plural "Based on: %2"
msgstr[0] "На аснове: %2"
msgstr[1] "На аснове: %2"
msgstr[2] "На аснове: %2"
msgstr[3] "На аснове: %2"

#: ui/main.qml:109
#, kde-format
msgctxt ""
"@label Precedes a list of buttons which can be clicked to preview the "
"theme's sounds. Keep it very short"
msgid "Preview:"
msgstr "Папярэдні прагляд:"

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview sound \"%1\""
msgstr "Папярэдняе праслухоўванне гуку \"%1\""

#: ui/main.qml:150
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview the demo sound for the theme \"%1\""
msgstr "Папярэдняе праслухоўванне дэманстрацыйнага гуку для тэмы \"%1\""

#: ui/main.qml:185
#, kde-format
msgctxt "%1 is a sound theme name. %2 is a sound name"
msgid "The %1 theme doesn't include the sound '%2'"
msgstr "Тэма %1 не ўключае гук \"%2\""

#: ui/main.qml:190
#, kde-format
msgid "Failed to preview sound: %1"
msgstr "Не ўдалося праслухаць гук: %1"
