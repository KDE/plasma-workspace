#: autostartmodel.cpp:383
#, kde-format
msgid "\"%1\" is not an absolute url."
msgstr "Το «%1» δεν είναι απόλυτο url."

#: autostartmodel.cpp:386
#, kde-format
msgid "\"%1\" does not exist."
msgstr "Το «%1» δεν υπάρχει."

#: autostartmodel.cpp:389
#, kde-format
msgid "\"%1\" is not a file."
msgstr "Το «%1» δεν είναι αρχείο."

#: autostartmodel.cpp:392
#, kde-format
msgid "\"%1\" is not readable."
msgstr "Το «%1» δεν είναι αναγνώσιμο."

#: ui/entry.qml:30
#, kde-format
msgctxt "@label Stop the Systemd unit for a running process"
msgid "Stop"
msgstr "Διακοπή"

#: ui/entry.qml:31
#, kde-format
msgctxt "@label Start the Systemd unit for a currently inactive process"
msgid "Start"
msgstr "Έναρξη"

#: ui/entry.qml:71
#, kde-format
msgctxt ""
"@label The name of a Systemd unit for an app or script that will autostart"
msgid "Name:"
msgstr "Όνομα:"

#: ui/entry.qml:77
#, kde-format
msgctxt ""
"@label The current status (e.g. active or inactive) of a Systemd unit for an "
"app or script that will autostart"
msgid "Status:"
msgstr "Κατάσταση:"

#: ui/entry.qml:83
#, kde-format
msgctxt ""
"@label A date and time follows this text, making a sentence like 'Last "
"activated on: August 7th 11 PM 2023'"
msgid "Last activated on:"
msgstr "Τελευταία ενεργοποίηση στις:"

#: ui/entry.qml:114
#, kde-format
msgid "Unable to load logs. Try refreshing."
msgstr "Αδυναμία φόρτωσης αρχείων καταγραφής. Δοκιμάστε ανανέωση."

#: ui/entry.qml:118
#, kde-format
msgctxt "@action:button Refresh entry logs when it failed to load"
msgid "Refresh"
msgstr "Ανανέωση"

#: ui/main.qml:35
#, kde-format
msgid "Make Executable"
msgstr "Δημιουργία εκτελέσιμου"

#: ui/main.qml:55
#, kde-format
msgid "The file '%1' must be executable to run at logout."
msgstr ""
"Το αρχείο «%1» πρέπει να είναι εκτελέσιμο για να εκτελεστεί κατά την "
"αποσύνδεση."

#: ui/main.qml:57
#, kde-format
msgid "The file '%1' must be executable to run at login."
msgstr ""
"Το αρχείο «%1» πρέπει να είναι εκτελέσιμο για να εκτελεστεί κατά την είσοδο."

#: ui/main.qml:68
#, kde-format
msgctxt "@action:button menu button; add new application etc"
msgid "Add New"
msgstr "Προσθήκη νέου"

#: ui/main.qml:73
#, kde-format
msgctxt "@action:inmenu completes: add new application"
msgid "Application…"
msgstr "Εφαρμογή…"

#: ui/main.qml:80
#, kde-format
msgctxt "@action:inmenu completes: add new login script"
msgid "Login Script…"
msgstr "Σενάριο εισόδου…"

#: ui/main.qml:87
#, kde-format
msgctxt "@action:inmenu completes: add new logout script"
msgid "Logout Script…"
msgstr "Σενάριο αποσύνδεσης…"

#: ui/main.qml:124
#, kde-format
msgid ""
"%1 has not been autostarted yet. Details will be available after the system "
"is restarted."
msgstr ""
"Το %1 δεν έχει ξεκινήσει ακόμα αυτόματα. Λεπτομέρειες θα είναι διαθέσιμες "
"μετά την επανεκκίνηση του συστήματος."

#: ui/main.qml:147
#, kde-format
msgctxt ""
"@label Entry hasn't been autostarted because system hasn't been restarted"
msgid "Not autostarted yet"
msgstr "Δεν έχει γίνει ακόμα αυτόματη εκκίνηση"

#: ui/main.qml:156
#, kde-format
msgctxt "@action:button"
msgid "See properties"
msgstr "Δείτε ιδιότητες"

#: ui/main.qml:167
#, kde-format
msgctxt "@action:button"
msgid "Remove entry"
msgstr "Αφαίρεση καταχώρησης"

#: ui/main.qml:183
#, kde-format
msgid "Applications"
msgstr "Εφαρμογές"

#: ui/main.qml:186
#, kde-format
msgid "Login Scripts"
msgstr "Σενάρια εισόδου"

#: ui/main.qml:189
#, kde-format
msgid "Pre-startup Scripts"
msgstr "Σενάρια προ εκκίνησης"

#: ui/main.qml:192
#, kde-format
msgid "Logout Scripts"
msgstr "Σενάρια αποσύνδεσης"

#: ui/main.qml:201
#, kde-format
msgid "No user-specified autostart items"
msgstr "Δεν υπάρχουν αντικείμενα αυτόματης εκκίνησης καθορισμένα από το χρήστη"

#: ui/main.qml:202
#, kde-kuit-format
msgctxt "@info 'some' refers to autostart items"
msgid "Click the <interface>Add New</interface> button to add some"
msgstr ""
"Πατήστε το κουμπί <interface>Προσθήκη νέου</interface> για να προσθέσετε "
"κάποιο"

#: ui/main.qml:217
#, kde-format
msgid "Choose Login Script"
msgstr "Επιλογή σεναρίου εισόδου"

#: ui/main.qml:237
#, kde-format
msgid "Choose Logout Script"
msgstr "Επιλογή σεναρίου αποσύνδεσης"

#: unit.cpp:26
#, kde-format
msgctxt "@label Entry is running right now"
msgid "Running"
msgstr "Εκτελείται"

#: unit.cpp:27
#, kde-format
msgctxt "@label Entry is not running right now (exited without error)"
msgid "Not running"
msgstr "Δεν εκτελείται"

#: unit.cpp:28
#, kde-format
msgctxt "@label Entry is being started"
msgid "Starting"
msgstr "Εκκινείται"

#: unit.cpp:29
#, kde-format
msgctxt "@label Entry is being stopped"
msgid "Stopping"
msgstr "Διακόπτεται"

#: unit.cpp:30
#, kde-format
msgctxt "@label Entry has failed (exited with an error)"
msgid "Failed"
msgstr "Απέτυχε"

#: unit.cpp:83
#, kde-format
msgid "Error occurred when receiving reply of GetAll call %1"
msgstr "Εμφανίστηκε σφάλμα κατά τη λήψη της απάντησης της κλήσης GetAll %1"

#: unit.cpp:155
#, kde-format
msgid "Failed to open journal"
msgstr "Αποτυχία ανοίγματος χρονολογίου"

