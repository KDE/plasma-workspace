#: main.cpp:118
#, kde-format
msgid "$HOME not set!"
msgstr "未設定 $HOME。"

#: main.cpp:122 main.cpp:128
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME 目錄（%1）不存在。"

#: main.cpp:124
#, kde-format
