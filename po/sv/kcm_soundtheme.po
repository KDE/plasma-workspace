#, kde-format
msgctxt "@info:tooltip"
msgid "Preview sound \"%1\""
msgstr "Lyssna på ljudet \"%1\""

#: ui/main.qml:150
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview the demo sound for the theme \"%1\""
msgstr "Lyssna på demonstrationsljudet för temat \"%1\""

#: ui/main.qml:185
#, kde-format
msgctxt "%1 is a sound theme name. %2 is a sound name"
msgid "The %1 theme doesn't include the sound '%2'"
msgstr "Temat %1 innehåller inte ljudet '%2'"

#: ui/main.qml:190
#, kde-format
msgid "Failed to preview sound: %1"
msgstr "Misslyckades lyssna på ljudet: %1"
