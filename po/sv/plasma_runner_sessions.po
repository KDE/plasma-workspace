#: sessionrunner.cpp:23
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to log out of the "
"session"
msgid "logout;log out"
msgstr "utloggning;logga ut"

#: sessionrunner.cpp:26
#, kde-format
msgid "Logs out, exiting the current desktop session"
msgstr "Loggar ut, och avslutar nuvarande skrivbordssession"

#: sessionrunner.cpp:33
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to shut down the "
"computer"
msgid "shutdown;shut down"
msgstr "avstängning;stäng av"

#: sessionrunner.cpp:36
#, kde-format
msgid "Turns off the computer"
msgstr "Stänger av datorn"

#: sessionrunner.cpp:43
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to restart the "
"computer"
msgid "restart;reboot"
msgstr "starta om;omstart"

#: sessionrunner.cpp:46
#, kde-format
msgid "Reboots the computer"
msgstr "Startar om datorn"

#: sessionrunner.cpp:54
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to lock the screen"
msgid "lock;lock screen"
msgstr "lås;lås skärmen"

#: sessionrunner.cpp:56
#, kde-format
msgid "Locks the current sessions and starts the screen saver"
msgstr "Låser nuvarande session och startar skärmsläckaren"

#: sessionrunner.cpp:63
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to save the "
"desktop session"
msgid "save;save session"
msgstr "spara;spara session"

#: sessionrunner.cpp:66
#, kde-format
msgid "Saves the current session for session restoration"
msgstr "Sparar den nuvarande sessionen för återställning"

#: sessionrunner.cpp:73
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to switch user "
"sessions"
msgid "switch user;new session"
msgstr "byt användare;ny session"

#: sessionrunner.cpp:76
#, kde-format
msgid "Starts a new session as a different user"
msgstr "Startar en ny session som en annan användare"

#: sessionrunner.cpp:83
#, kde-format
msgctxt "KRunner keyword to list user sessions"
msgid "sessions"
msgstr "sessioner"

#: sessionrunner.cpp:84
#, kde-format
msgid "Lists all sessions"
msgstr "Lista alla sessioner"

#: sessionrunner.cpp:87
#, kde-format
msgctxt "KRunner keyword to switch user sessions"
msgid "switch"
msgstr "byt"

#: sessionrunner.cpp:89
#, kde-format
msgid ""
"Switches to the active session for the user :q:, or lists all active "
"sessions if :q: is not provided"
msgstr ""
"Byter till aktiv session för användaren :q:, eller listar alla aktiva "
"sessioner om :q: inte tillhandahålls"

