#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "Encerramento cancelado por '%1'"

#: main.cpp:118
#, kde-format
msgid "$HOME not set!"
msgstr "A $HOME não está definida!"

#: main.cpp:122 main.cpp:128
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "A pasta da $HOME (%1) não existe."

#: main.cpp:124
#, kde-format
msgid "No write access to $HOME directory (%1)."
msgstr "Não tem acesso de escrita para a pasta da $HOME (%1)."

#: main.cpp:130
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "Não tem acesso de leitura para a pasta da $HOME (%1)."

#: main.cpp:134
#, kde-format
msgid "$HOME directory (%1) is out of disk space."
msgstr "A pasta da $HOME (%1) não tem espaço livre em disco."

#: main.cpp:137
#, kde-format
msgid "Writing to the $HOME directory (%2) failed with the error '%1'"
msgstr "A gravação na pasta da $HOME (%2) falhou com o erro '%1'"

#: main.cpp:150 main.cpp:185
#, kde-format
msgid "No write access to '%1'."
msgstr "Não tem acesso de escrita em '%1'."

#: main.cpp:152 main.cpp:187
#, kde-format
msgid "No read access to '%1'."
msgstr "Não tem acesso de leitura em '%1'."

#: main.cpp:160 main.cpp:173
#, kde-format
msgid "Temp directory (%1) is out of disk space."
msgstr "A pasta temporária (%1) não tem espaço livre no disco."

#: main.cpp:163 main.cpp:176
#, kde-format
msgid ""
"Writing to the temp directory (%2) failed with\n"
"    the error '%1'"
msgstr ""
"A gravação na pasta temporária (%2) falhou com\n"
"o erro '%1'"

#: main.cpp:191
#, kde-format
msgid ""
"The following installation problem was detected\n"
"while trying to start Plasma:"
msgstr ""
"Foi detectado o seguinte problema de instalação\n"
"ao tentar iniciar o Plasma:"

#: main.cpp:194
#, kde-format
msgid ""
"\n"
"\n"
"Plasma is unable to start.\n"
msgstr ""
"\n"
"\n"
"O Plasma não consegue iniciar.\n"

#: main.cpp:201
#, kde-format
msgid "Plasma Workspace installation problem!"
msgstr "Problema de instalação na Área de Trabalho do Plasma!"

#: main.cpp:237
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"O gestor de sessões do Plasma que sabe falar o protocolo normalizado\n"
"de gestão de sessões do X11R6 (XSMP)."

#: main.cpp:241
#, kde-format
msgid "Restores the saved user session if available"
msgstr "Restaura a sessão gravada do utilizador, se disponível"

#: main.cpp:244
#, kde-format
msgid "Also allow remote connections"
msgstr "Permitir também as ligações remotas"

#: main.cpp:247
#, kde-format
msgid "Starts the session in locked mode"
msgstr "Inicia a sessão no modo bloqueado"

#: main.cpp:251
#, kde-format
msgid ""
"Starts without lock screen support. Only needed if other component provides "
"the lock screen."
msgstr ""
"Inicia sem o suporte para o bloqueio do ecrã. Só é necessário se outro "
"componente oferecer o ecrã de bloqueio."

#: server.cpp:853
#, kde-format
msgid "Session Management"
msgstr "Gestão de Sessões"

#: server.cpp:856
#, kde-format
msgid "Log Out"
msgstr "Sair"

#: server.cpp:861
#, kde-format
msgid "Log Out Without Confirmation"
msgstr "Sair sem Confirmação"

#: server.cpp:866
#, kde-format
msgid "Halt Without Confirmation"
msgstr "Encerrar sem Confirmação"

#: server.cpp:871
#, kde-format
msgid "Reboot Without Confirmation"
msgstr "Reiniciar sem Confirmação"
