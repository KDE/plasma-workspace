#: logout.cpp:273
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "«%1» ha cancel·lat la sortida"

#: logout.cpp:371
#, kde-format
msgctxt "@info:status"
msgid "Session is exiting"
msgstr "La sessió està finalitzant"

#: main.cpp:72 main.cpp:80
