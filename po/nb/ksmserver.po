#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "Utlogging avbrutt av «%1»."

#: main.cpp:118
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME ikke oppgitt."

#: main.cpp:122 main.cpp:128
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME-mappa (%1) finnes ikke."

#: main.cpp:124
#, kde-format
msgid "No write access to $HOME directory (%1)."
msgstr "Ingen skrivetilgang til $HOME-mappa (%1),"

#: main.cpp:130
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "Ingen lesetilgang til $HOME-mappa (%1)."

#: main.cpp:134
#, kde-format
msgid "$HOME directory (%1) is out of disk space."
msgstr "$HOME-mappa (%1) har ikke mer ledig diskplass."

#: main.cpp:137
#, kde-format
msgid "Writing to the $HOME directory (%2) failed with the error '%1'"
msgstr "Klarte ikke skrive til $HOME-mappa (%2): «%1»"

#: main.cpp:150 main.cpp:185
#, kde-format
msgid "No write access to '%1'."
msgstr "Ingen skrivetilgang til «%1»."

#: main.cpp:152 main.cpp:187
#, kde-format
msgid "No read access to '%1'."
msgstr "Ingen lesetilgang til «%1»."

#: main.cpp:160 main.cpp:173
#, kde-format
msgid "Temp directory (%1) is out of disk space."
msgstr "Den midlertidige mappa (%1) har ikke mer ledig diskplass."

#: main.cpp:163 main.cpp:176
#, kde-format
msgid ""
"Writing to the temp directory (%2) failed with\n"
"    the error '%1'"
msgstr ""
"Klarte ikke skrive til mappa for midlertidige filer (%2)\n"
"    Feilmelding: «%1»"

#: main.cpp:191
#, kde-format
msgid ""
"The following installation problem was detected\n"
"while trying to start Plasma:"
msgstr ""
"Følgende installasjonsproblem ble funnet ved\n"
"forsøk på start av Plasma:"

#: main.cpp:194
#, kde-format
msgid ""
"\n"
"\n"
"Plasma is unable to start.\n"
msgstr ""
"\n"
"\n"
"Kan ikke starte Plasma .\n"

#: main.cpp:201
#, kde-format
msgid "Plasma Workspace installation problem!"
msgstr "Feil ved installasjon av Plasma-arbeidsområde"

#: main.cpp:237
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"Den pålitelige øktbehandleren til Plasma som snakker med standarden\n"
"øktbehandlingsprotokollen til X11R6 (XSMP)."

#: main.cpp:241
#, kde-format
msgid "Restores the saved user session if available"
msgstr "Gjenoppretter tilstanden fra den forrige økta, hvis tilgjengelig"

#: main.cpp:244
#, kde-format
msgid "Also allow remote connections"
msgstr "Tillat også nettverkstilkoblinger"

#: main.cpp:247
#, kde-format
msgid "Starts the session in locked mode"
msgstr "Starter økta som låst"

#: main.cpp:251
#, kde-format
msgid ""
"Starts without lock screen support. Only needed if other component provides "
"the lock screen."
msgstr ""
"Starter uten støtte for skjermlåsing. Bare nødvendig hvis andre komponenter "
"står for skjermlåsinga."

#: server.cpp:857
#, kde-format
msgid "Session Management"
msgstr "Øktbehandling"

#: server.cpp:860
#, kde-format
msgid "Log Out"
msgstr "Logg ut"

#: server.cpp:865
#, kde-format
msgid "Log Out Without Confirmation"
msgstr "Logg ut uten bekreftelse"

#: server.cpp:870
#, kde-format
msgid "Halt Without Confirmation"
msgstr "Stopp datamaskinen uten bekreftelse"

#: server.cpp:875
#, kde-format
