msgstr[0] "%1 分钟"

#: ui/main.qml:407
#, kde-format
msgid "Input minutes - min. 1, max. 600"
msgstr "输入分钟，最小为 1，最大为 600"

#: ui/main.qml:426
#, kde-format
msgid "Error: Transition time overlaps."
msgstr "错误：过渡时间重叠。"

#: ui/main.qml:449
#, kde-format
msgctxt "@info:placeholder"
msgid "Locating…"
msgstr "正在定位…"

