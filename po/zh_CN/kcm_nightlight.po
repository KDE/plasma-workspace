#, kde-format
msgid "Night light temperature:"
msgstr "夜间色温："

#: ui/main.qml:285
#, kde-format
msgid "Switching times:"
msgstr "切换时间："

#: ui/main.qml:288
#, kde-format
msgid "Always off"
msgstr "总是关闭"

#: ui/main.qml:289
#, kde-format
msgid "Sunset and sunrise at current location"
msgstr "当前地理位置的日落到日出"

#: ui/main.qml:290
#, kde-format
msgid "Sunset and sunrise at manual location"
msgstr "手动选定的地理位置的日落到日出"

#: ui/main.qml:291
#, kde-format
msgid "Custom times"
msgstr "自定义时间"

#: ui/main.qml:292
#, kde-format
