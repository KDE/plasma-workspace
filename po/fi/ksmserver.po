#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "”%1” perui uloskirjautumisen."

#: main.cpp:77
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME-muuttujaa ei ole asetettu!"

#: main.cpp:81 main.cpp:89
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME-kansiota (%1) ei ole olemassa."

#: main.cpp:84
#, kde-kuit-format
msgctxt "@info"
msgid ""
"No write access to $HOME directory (%1). If this is intentional, set "
"<envar>KDE_HOME_READONLY=1</envar> in your environment."
msgstr ""

#: main.cpp:91
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "$HOME-kansioon (%1) ei ole lukuoikeutta."

#: main.cpp:95
#, kde-format
msgid "$HOME directory (%1) is out of disk space."
msgstr "$HOME-kansiossa (%1) ei ole vapaata levytilaa."

#: main.cpp:98
#, kde-format
msgid "Writing to the $HOME directory (%2) failed with the error '%1'"
msgstr "$HOME-kansioon (%2) kirjoittaminen epäonnistui virheellä ”%1”"

#: main.cpp:111 main.cpp:146
#, kde-format
msgid "No write access to '%1'."
msgstr "Kansioon ”%1” ei ole kirjoitusoikeutta."

#: main.cpp:113 main.cpp:148
#, kde-format
msgid "No read access to '%1'."
msgstr "Kansioon ”%1” ei ole lukuoikeutta."

#: main.cpp:121 main.cpp:134
#, kde-format
msgid "Temp directory (%1) is out of disk space."
msgstr "Väliaikaiskansiossa (%1) ei ole vapaata levytilaa."

#: main.cpp:124 main.cpp:137
#, kde-format
msgid ""
"Writing to the temp directory (%2) failed with\n"
"    the error '%1'"
msgstr ""
"Väliaikaiskansioon (%2) kirjoittaminen epäonnistui\n"
"virheellä ”%1”"

#: main.cpp:152
#, kde-format
msgid ""
"The following installation problem was detected\n"
"while trying to start Plasma:"
msgstr ""
"Plasmaa käynnistettäessä havaittiin\n"
"seuraava asennusongelma:"

#: main.cpp:155
#, kde-format
msgid ""
"\n"
"\n"
"Plasma is unable to start.\n"
msgstr ""
"\n"
"\n"
"Plasmaa ei voi käynnistää.\n"

#: main.cpp:162
#, kde-format
msgid "Plasma Workspace installation problem!"
msgstr "Plasma-työtilan asennusongelma!"

#: main.cpp:196
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"Standardinmukaista X11R6-istunnonhallintayhteyskäytäntöä (XSMP)\n"
"käyttävä luotettava Plasma-istunnonhallinta."

#: main.cpp:200
#, kde-format
msgid "Restores the saved user session if available"
msgstr "Palauttaa tallennetun käyttäjän istunnon, jos mahdollista"

#: main.cpp:203
#, kde-format
msgid "Also allow remote connections"
msgstr "Salli myös etäyhteydet"

#: main.cpp:206
#, kde-format
msgid "Starts the session in locked mode"
msgstr "Aloittaa istunnon lukittuna"

#: main.cpp:210
#, kde-format
msgid ""
"Starts without lock screen support. Only needed if other component provides "
"the lock screen."
msgstr ""
"Käynnistyy ilman lukitusnäyttötukea. Tarvitaan vain, jos jokin toinen "
"komponentti tarjoaa lukitusnäytön."

