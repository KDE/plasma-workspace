msgstr "guardar;guardar sesión"

#: sessionrunner.cpp:47
#, kde-format
msgid "Saves the current session for session restoration"
msgstr "Guarda la sesión actual para la restauración de sesiones"

#: sessionrunner.cpp:50
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to switch user "
"sessions"
msgid "switch user;new session"
msgstr "cambiar usuario;nueva sesión"

#: sessionrunner.cpp:53
#, kde-format
msgid "Switch to a session as a different user"
msgstr "Cambiar a una sesión como otro usuario"

