#: module.cpp:68
#, kde-format
msgctxt "@title"
msgid "Memory Shortage Avoided"
msgstr "Memoria urritasuna saihestu da"

#: module.cpp:71
#, kde-kuit-format
msgctxt "@info"
msgid ""
"<command>%1</command> has been terminated by the Linux kernel because the "
"system is low on memory. Consider closing unused applications or browser "
"tabs."
msgstr ""
"Linux-en muinak <command>%1</command> bukatu du, sistema memoriarik gabe "
"gelditzen ari delako. Kontuan hartu erabili gabeko aplikazioak edo "
"arakatzailearen fitxak ixteko aukera."

#: module.cpp:77
#, kde-kuit-format
msgctxt "@info"
msgid ""
"The background service <command>%1</command> has been terminated by the "
"Linux kernel because the system is low on memory. Consider closing unused "
"applications or browser tabs."
msgstr ""
"Linux-en muinak atzeko planoko <command>%1</command> zerbitzua bukatu du, "
"sistemari memoria agortzen ari zaiolako. Kontuan hartu erabili gabeko "
"aplikazioak edo arakatzailearen fitxak ixteko aukera."

#: module.cpp:85
#, kde-format
msgctxt "@action"
msgid "Restart Application"
msgstr "Aplikazioa berrabiarazi"
