msgstr "Erakutsi laster-leihoa gailu berria konektatzen denean"

#: plugin/ksolidnotify.cpp:190
#, kde-format
msgid "Device Status"
msgstr "Gailuaren egoera"

#: plugin/ksolidnotify.cpp:190
#, kde-format
msgid "A device can now be safely removed"
msgstr "Orain, gailu bat modu seguruan ken daiteke"

#: plugin/ksolidnotify.cpp:191
#, kde-format
msgid "This device can now be safely removed."
msgstr "Orain, gailu hori modu seguruan ken daiteke."

#: plugin/ksolidnotify.cpp:198
#, kde-format
msgid "You are not authorized to mount this device."
msgstr "Ez zaude gailu hori muntatzera baimenduta."

#: plugin/ksolidnotify.cpp:201
#, kde-format
msgctxt "Remove is less technical for unmount"
msgid "You are not authorized to remove this device."
msgstr "Ez zaude gailu hori kentzera baimenduta."

#: plugin/ksolidnotify.cpp:204
#, kde-format
msgid "You are not authorized to eject this disc."
msgstr "Ez zaude disko hori egoztera baimenduta."

#: plugin/ksolidnotify.cpp:211
#, kde-format
msgid "Could not mount this device as it is busy."
msgstr "Ezin izan da gailu hori muntatu lanpetuta dagoelako."

#: plugin/ksolidnotify.cpp:242
#, kde-format
msgid "One or more files on this device are open within an application."
msgstr "Gailu horretako fitxategi bat gutxienez aplikazio batean irekita dago."

#: plugin/ksolidnotify.cpp:244
#, kde-format
msgid "One or more files on this device are opened in application \"%2\"."
msgid_plural ""
"One or more files on this device are opened in following applications: %2."
msgstr[0] ""
"Gailu horretako fitxategi bat gutxienez \"%2\" aplikazioan irekita daude."
msgstr[1] ""
"Gailu horretako fitxategi bat gutxienez ondoko aplikazioetan irekita dago: %2"

#: plugin/ksolidnotify.cpp:247
#, kde-format
msgctxt "separator in list of apps blocking device unmount"
msgid ", "
msgstr ", "

#: plugin/ksolidnotify.cpp:265
#, kde-format
msgid "Could not mount this device."
msgstr "Ezin izan du gailu hori muntatu."

#: plugin/ksolidnotify.cpp:268
#, kde-format
msgctxt "Remove is less technical for unmount"
msgid "Could not remove this device."
msgstr "Ezin izan du gailu hori kendu."

#: plugin/ksolidnotify.cpp:271
#, kde-format
msgid "Could not eject this disc."
msgstr "Ezin izan du disko hori egotzi."

