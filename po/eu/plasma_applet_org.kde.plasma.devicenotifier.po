#, kde-format
msgid "Most Recent Device"
msgstr "Konektatutako azken gailua"

#: package/contents/ui/main.qml:57
#, kde-format
msgid "No Devices Available"
msgstr "Ez dago gailu eskuragarririk"

#: package/contents/ui/main.qml:207
#, kde-format
msgctxt "Open auto mounter kcm"
msgid "Configure Removable Devices…"
msgstr "Konfiguratu gailu aldagarriak..."

#: package/contents/ui/main.qml:233
#, kde-format
msgid "Removable Devices"
msgstr "Gailu aldagarriak"

#: package/contents/ui/main.qml:248
#, kde-format
msgid "Non Removable Devices"
msgstr "Gailu ez-aldagarriak"

