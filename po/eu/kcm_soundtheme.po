#, kde-format
msgctxt "@info:tooltip"
msgid "Preview sound \"%1\""
msgstr "Aurrez ikusi \"%1\" soinua"

#: ui/main.qml:150
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview the demo sound for the theme \"%1\""
msgstr "Aurrez ikusi \"%1\" gaiaren demo-soinua"

#: ui/main.qml:185
#, kde-format
msgctxt "%1 is a sound theme name. %2 is a sound name"
msgid "The %1 theme doesn't include the sound '%2'"
msgstr "%1 gaiak ez dauka '%2' soinua"

#: ui/main.qml:190
#, kde-format
msgid "Failed to preview sound: %1"
msgstr "Soinua aurrez ikustea huts egin du: %1"
