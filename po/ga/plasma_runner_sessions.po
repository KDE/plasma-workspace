#: sessionrunner.cpp:20
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to log out of the "
"session"
msgid "logout;log out"
msgstr "logáil amach"

#: sessionrunner.cpp:23
#, kde-format
msgid "Logs out, exiting the current desktop session"
msgstr "Logáil amach agus scoir den seisiún reatha deisce"

#: sessionrunner.cpp:27
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to shut down the "
"computer"
msgid "shutdown;shut down;power;poweroff;power off"
msgstr "múch;múchta;cumhacht;cumhacht a mhúchadh"

#: sessionrunner.cpp:30
#, kde-format
msgid "Turns off the computer"
msgstr "Múch an ríomhaire"

#: sessionrunner.cpp:33
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to restart the "
"computer"
msgid "restart;reboot"
msgstr "atosaigh"

#: sessionrunner.cpp:36
#, kde-format
msgid "Reboots the computer"
msgstr "Atosaigh an ríomhaire"

#: sessionrunner.cpp:40
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to lock the screen"
msgid "lock;lock screen"
msgstr "glasáil;glas;glasáil an scáileán;scáileán glasála"

#: sessionrunner.cpp:42
#, kde-format
msgid "Locks the current sessions and starts the screen saver"
msgstr "Cuir na seisiúin reatha faoi ghlas agus tosaigh spárálaí an scáileáin"

#: sessionrunner.cpp:45
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to save the "
"desktop session"
msgid "save;save session"
msgstr "sábháil;sábháil seisiún"

#: sessionrunner.cpp:48
#, kde-format
msgid "Saves the current session for session restoration"
msgstr "Sábháiltear an seisiún reatha deisce chun seisiún aischur"

#: sessionrunner.cpp:51
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to switch user "
"sessions"
msgid "switch user;new session"
msgstr "athraigh an t-úsáideoir;seisiún nua"

#: sessionrunner.cpp:54
#, kde-format
msgid "Switch to a session as a different user"
msgstr "Athraigh chun seisiún nua mar úsáideoir eile"

#: sessionrunner.cpp:75
#, kde-format
msgctxt "log out command"
msgid "Log Out"
msgstr "Logáil Amach"

#: sessionrunner.cpp:83
#, kde-format
msgctxt "turn off computer command"
msgid "Shut Down"
msgstr "Múch"

#: sessionrunner.cpp:90
#, kde-format
msgctxt "restart computer command"
msgid "Restart"
msgstr "Atosaigh"

#: sessionrunner.cpp:97
#, kde-format
msgctxt "lock screen command"
msgid "Lock"
msgstr "Glasáil"

#: sessionrunner.cpp:104
#, kde-format
msgid "Save Session"
msgstr "Sábháil Seisiún"

#: sessionrunner.cpp:111
#, kde-format
msgid "Switch User"
msgstr "Athraigh an t-Úsáideoir"
