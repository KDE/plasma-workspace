#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "'%1' katkestas väljalogimise"

#: main.cpp:118
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME pole määratud!"

#: main.cpp:122 main.cpp:128
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME kataloogi (%1) ei ole olemas."

#: main.cpp:124
#, kde-format
msgid "No write access to $HOME directory (%1)."
msgstr "$HOME kataloogis (%1) puudub kirjutamisõigus."

#: main.cpp:130
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "$HOME kataloogis (%1) puudub lugemisõigus."

#: main.cpp:134
#, kde-format
msgid "$HOME directory (%1) is out of disk space."
msgstr "$HOME kataloogis (%1) on ruum otsa lõppenud."

#: main.cpp:137
#, kde-format
msgid "Writing to the $HOME directory (%2) failed with the error '%1'"
msgstr "Kirjutamine $HOME kataloogi (%2) lõppes veaga '%1'"

#: main.cpp:150 main.cpp:185
#, kde-format
msgid "No write access to '%1'."
msgstr "Kataloogis \"%1\" puudub kirjutamisõigus."

#: main.cpp:152 main.cpp:187
#, kde-format
msgid "No read access to '%1'."
msgstr "Kataloogis \"%1\" puudub lugemisõigus."

#: main.cpp:160 main.cpp:173
#, kde-format
msgid "Temp directory (%1) is out of disk space."
msgstr "Ajutises kataloogis (%1) on ruum otsa lõppenud."

#: main.cpp:163 main.cpp:176
#, kde-format
msgid ""
"Writing to the temp directory (%2) failed with\n"
"    the error '%1'"
msgstr ""
"Kirjutamine ajutisse kataloogi (%2)\n"
"    lõppes veaga '%1'"

#: main.cpp:191
#, kde-format
msgid ""
"The following installation problem was detected\n"
"while trying to start Plasma:"
msgstr ""
"Plasma käivitamise ajal tuvastati\n"
"järgmine paigaldusprobleem:"

#: main.cpp:194
#, kde-format
msgid ""
"\n"
"\n"
"Plasma is unable to start.\n"
msgstr ""
"\n"
"\n"
"Plasma käivitamine ei õnnestunud.\n"

#: main.cpp:201
#, kde-format
msgid "Plasma Workspace installation problem!"
msgstr "Plasma töötsooni paigalduse probleem!"

#: main.cpp:237
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"Töökindel Plasma seansihaldur, mis suhtleb standardse X11R6 \n"
"seansihalduse protokolli (XSMP) abil."

#: main.cpp:241
#, kde-format
msgid "Restores the saved user session if available"
msgstr "Võimaluse korral taastab eelmise seansi"

#: main.cpp:244
#, kde-format
msgid "Also allow remote connections"
msgstr "Lubatud on ka kaugühendused"

#: main.cpp:247
#, kde-format
msgid "Starts the session in locked mode"
msgstr "Seansi käivitamine lukustatud režiimis"

#: main.cpp:251
#, kde-format
msgid ""
"Starts without lock screen support. Only needed if other component provides "
"the lock screen."
msgstr ""
"Käivitamine lukustusekraani toetuseta. Vajalik ainult siis, kui mõni muu "
"komponent pakub lukustusekraani."

#: server.cpp:857
#, kde-format
msgid "Session Management"
msgstr "Seansihaldur"

#: server.cpp:860
#, kde-format
msgid "Log Out"
msgstr "Väljalogimine"

#: server.cpp:865
#, kde-format
