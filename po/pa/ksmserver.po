#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "ਲਾਗਆਉਟ '%1' ਵਲੋਂ ਰੱਦ ਕੀਤਾ ਗਿਆ"

#: main.cpp:118
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME ਸੈੱਟ ਨਹੀਂ ਹੈ!"

#: main.cpp:122 main.cpp:128
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME ਡਾਇਰੈਕਟਰੀ (%1) ਮੌਜੂਦ ਨਹੀਂ ਹੈ।"

#: main.cpp:124
#, kde-format
msgid "No write access to $HOME directory (%1)."
msgstr "$HOME (%1) ਲਈ ਕੋਈ ਲਿਖਣ ਪਹੁੰਚ ਨਹੀਂ ਹੈ।"

#: main.cpp:130
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "$HOME (%1) ਲਈ ਕੋਈ ਪੜ੍ਹਨ ਪਹੁੰਚ ਨਹੀਂ ਹੈ।"

#: main.cpp:134
#, kde-format
msgid "$HOME directory (%1) is out of disk space."
msgstr "$HOME ਡਾਇਰੈਕਟਰੀ (%1) ਲਈ ਡਿਸਕ ਥਾਂ ਭਰ ਗਈ ਹੈ।"

#: main.cpp:137
#, kde-format
msgid "Writing to the $HOME directory (%2) failed with the error '%1'"
msgstr "'%1' ਗ਼ਲਤੀ ਨਾਲ $HOME ਡਾਇਰੈਕਟਰੀ (%2) ਵਿੱਚ ਲਿਖਣਾ ਅਸਫ਼ਲ ਹੈ"

#: main.cpp:150 main.cpp:185
#, kde-format
msgid "No write access to '%1'."
msgstr "'%1' ਲਈ ਕੋਈ ਲਿਖਣ ਅਧਿਕਾਰ ਨਹੀਂ ਹੈ।"

#: main.cpp:152 main.cpp:187
#, kde-format
msgid "No read access to '%1'."
msgstr "'%1' ਲਈ ਕੋਈ ਪੜ੍ਹਨ ਪਹੁੰਚ ਨਹੀਂ ਹੈ।"

#: main.cpp:160 main.cpp:173
#, kde-format
msgid "Temp directory (%1) is out of disk space."
msgstr "ਆਰਜ਼ੀ ਡਾਇਰੈਕਟਰੀ (%1) ਲਈ ਡਿਸਕ ਥਾਂ ਭਰ ਗਈ ਹੈ।"

#: main.cpp:163 main.cpp:176
#, kde-format
msgid ""
"Writing to the temp directory (%2) failed with\n"
"    the error '%1'"
msgstr ""
"'%1' ਗ਼ਲਤੀ ਨਾਲ ਆਰਜ਼ੀ ਡਾਇਰੈਕਟਰੀ (%2) ਵਿੱਚ ਲਿਖਣਾ\n"
"   ਅਸਫ਼ਲ ਹੈ"

#: main.cpp:191
#, kde-format
msgid ""
"The following installation problem was detected\n"
"while trying to start Plasma:"
msgstr ""

#: main.cpp:194
#, kde-format
msgid ""
"\n"
"\n"
"Plasma is unable to start.\n"
msgstr ""
"\n"
"\n"
"ਪਲਾਜ਼ਮਾ ਸ਼ੁਰੂ ਹੋਣ ਲਈ ਅਸਫ਼ਲ ਹੈ।\n"

#: main.cpp:201
#, kde-format
msgid "Plasma Workspace installation problem!"
msgstr "ਪਲਾਜ਼ਮਾ ਵਰਕਸਪੇਸ ਇੰਸਟਾਲੇਸ਼ਨ ਦੀ ਸਮੱਸਿਆ ਹੈ!"

#: main.cpp:237
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"ਇੱਕ ਭਰੋਸੇਯੋਗ ਪਲਾਜ਼ਮਾ ਸ਼ੈਸ਼ਨ ਮੈਨੇਜਰ, ਜੋ ਕਿ ਸਟੈਂਡਰਡ X11R6 \n"
"ਸ਼ੈਸ਼ਨ ਪਰਬੰਧ ਪਰੋਟੋਕਾਲ (XSMP) ਨਾਲ ਸੰਚਾਰ ਕਰਦਾ ਹੈ।"

#: main.cpp:241
#, kde-format
msgid "Restores the saved user session if available"
msgstr "ਵਰਤੋਂਕਾਰ ਦਾ, ਜੇ ਸੰਭਾਲਿਆ ਸ਼ੈਸ਼ਨ ਹੈ, ਤਾਂ ਮੁੜ-ਸਟੋਰ ਕਰੋ"

#: main.cpp:244
#, kde-format
msgid "Also allow remote connections"
msgstr "ਰਿਮੋਟ ਕੁਨੈਕਸ਼ਨ ਵੀ ਮਨਜ਼ੂਰ ਹਨ"

#: main.cpp:247
#, kde-format
msgid "Starts the session in locked mode"
msgstr "ਸ਼ੈਸ਼ਨ ਲਾਕ ਕੀਤੇ ਮੋਡ ਵਿੱਚ ਸ਼ੁਰੂ ਕਰੋ"

#: main.cpp:251
#, kde-format
msgid ""
"Starts without lock screen support. Only needed if other component provides "
"the lock screen."
msgstr ""

#: server.cpp:853
#, kde-format
msgid "Session Management"
msgstr "ਸ਼ੈਸ਼ਨ ਦਾ ਇੰਤਜ਼ਾਮ"

#: server.cpp:856
#, kde-format
msgid "Log Out"
msgstr "ਲਾਗ ਆਉਟ"

#: server.cpp:861
#, kde-format
msgid "Log Out Without Confirmation"
msgstr "ਬਿਨਾਂ ਪੁੱਛੇ ਲਾਗ ਆਉਟ ਕਰੋ"

#: server.cpp:866
#, kde-format
msgid "Halt Without Confirmation"
msgstr "ਬਿਨਾਂ ਪੁੱਛੇ ਬੰਦ ਕਰੋ"

#: server.cpp:871
#, kde-format
msgid "Reboot Without Confirmation"
msgstr "ਬਿਨਾਂ ਪੁੱਛੇ ਮੁੜ-ਚਾਲੂ ਕਰੋ"

