#: kcm_soundtheme.cpp:111
#, kde-format
msgctxt "Name of the fallback \"freedesktop\" sound theme"
msgid "FreeDesktop"
msgstr "FreeDesktop इति"

#: kcm_soundtheme.cpp:112
#, kde-format
msgid "Fallback sound theme from freedesktop.org"
msgstr "freedesktop.org इत्यस्मात् Fallback ध्वनिविषयः"

#: ui/main.qml:29
#, kde-format
msgctxt "@option:check"
msgid "Enable notification sounds"
msgstr "सूचनाध्वनयः सक्षमाः कुर्वन्तु"

#: ui/main.qml:96
#, kde-format
msgctxt ""
"%2 is a theme name or a list of theme names that the current theme inherits "
"from"
msgid "Based on: %2"
msgid_plural "Based on: %2"
msgstr[0] "आधारितम् : %2"
msgstr[1] "आधारितम् : %2"

#: ui/main.qml:109
#, kde-format
msgctxt ""
"@label Precedes a list of buttons which can be clicked to preview the "
"theme's sounds. Keep it very short"
msgid "Preview:"
msgstr "पूर्वावलोकनम् :"

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview sound \"%1\""
msgstr "पूर्वावलोकनध्वनि \"%1\"।"

#: ui/main.qml:150
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview the demo sound for the theme \"%1\""
msgstr "\"%1\" विषयस्य कृते डेमो ध्वनिं पूर्वावलोकनं कुर्वन्तु ।"

#: ui/main.qml:189
#, kde-format
msgctxt "%1 is a sound theme name. %2 is a sound name"
msgid "The %1 theme doesn't include the sound '%2'"
msgstr "%1 विषये '%2' इति ध्वनिः न समाविष्टः ।"

#: ui/main.qml:194
#, kde-format
msgid "Failed to preview sound: %1"
msgstr "ध्वनिं पूर्वावलोकनं कर्तुं असफलम्: %1"
