#: autostartmodel.cpp:383
#, kde-format
msgid "\"%1\" is not an absolute url."
msgstr "\"%1\" निरपेक्षं url नास्ति ।"

#: autostartmodel.cpp:386
#, kde-format
msgid "\"%1\" does not exist."
msgstr "\"%1\" नास्ति ।"

#: autostartmodel.cpp:389
#, kde-format
msgid "\"%1\" is not a file."
msgstr "\"%1\" इति सञ्चिका नास्ति ।"

#: autostartmodel.cpp:392
#, kde-format
msgid "\"%1\" is not readable."
msgstr "\"%1\" पठनीयं नास्ति ।"

#: ui/entry.qml:30
#, kde-format
msgctxt "@label Stop the Systemd unit for a running process"
msgid "Stop"
msgstr "विरमतु"

#: ui/entry.qml:31
#, kde-format
msgctxt "@label Start the Systemd unit for a currently inactive process"
msgid "Start"
msgstr "प्रारंभः"

#: ui/entry.qml:71
#, kde-format
msgctxt ""
"@label The name of a Systemd unit for an app or script that will autostart"
msgid "Name:"
msgstr "नामः:"

#: ui/entry.qml:77
#, kde-format
msgctxt ""
"@label The current status (e.g. active or inactive) of a Systemd unit for an "
"app or script that will autostart"
msgid "Status:"
msgstr "स्थितिः:"

#: ui/entry.qml:83
#, kde-format
msgctxt ""
"@label A date and time follows this text, making a sentence like 'Last "
"activated on: August 7th 11 PM 2023'"
msgid "Last activated on:"
msgstr "अन्तिमवारं सक्रियम् अभवत् :"

#: ui/entry.qml:114
#, kde-format
msgid "Unable to load logs. Try refreshing."
msgstr "लॉग्स् लोड् कर्तुं असमर्थः। ताजगीं कर्तुं प्रयतस्व।"

#: ui/entry.qml:118
#, kde-format
msgctxt "@action:button Refresh entry logs when it failed to load"
msgid "Refresh"
msgstr "ताजगी"

#: ui/main.qml:35
#, kde-format
msgid "Make Executable"
msgstr "कार्यान्वयनीयं कुरुत"

#: ui/main.qml:55
#, kde-format
msgid "The file '%1' must be executable to run at logout."
msgstr "'%1' इति सञ्चिका लॉगआउट् करणसमये चालयितुं कार्यान्वयनीयं भवितुमर्हति ।"

#: ui/main.qml:57
#, kde-format
msgid "The file '%1' must be executable to run at login."
msgstr "प्रवेशसमये चालयितुं '%1' इति सञ्चिका कार्यान्वयनीयं भवितुमर्हति ।"

#: ui/main.qml:68
#, kde-format
msgctxt "@action:button menu button; add new application etc"
msgid "Add New"
msgstr "New योजयतु"

#: ui/main.qml:75
#, kde-format
msgctxt "@action:inmenu completes: add new application"
msgid "Application…"
msgstr "अनुप्रयोगः…"

#: ui/main.qml:82
#, kde-format
msgctxt "@action:inmenu completes: add new login script"
msgid "Login Script…"
msgstr "लॉगिन स्क्रिप्ट..."

#: ui/main.qml:89
#, kde-format
msgctxt "@action:inmenu completes: add new logout script"
msgid "Logout Script…"
msgstr "लॉगआउट स्क्रिप्ट..."

#: ui/main.qml:126
#, kde-format
msgid ""
"%1 has not been autostarted yet. Details will be available after the system "
"is restarted."
msgstr ""
"%1 अद्यापि स्वतः प्रारब्धः नास्ति । प्रणाली पुनः आरब्धस्य अनन्तरं विवरणं उपलब्धं भविष्यति।"

#: ui/main.qml:149
#, kde-format
msgctxt ""
"@label Entry hasn't been autostarted because system hasn't been restarted"
msgid "Not autostarted yet"
msgstr "अद्यापि स्वयमेव आरब्धं नास्ति"

#: ui/main.qml:158
#, kde-format
msgctxt "@action:button"
msgid "See properties"
msgstr "गुणाः पश्यन्तु"

#: ui/main.qml:169
#, kde-format
msgctxt "@action:button"
msgid "Remove entry"
msgstr "प्रविष्टिं निष्कासयन्तु"

#: ui/main.qml:185
#, kde-format
msgid "Applications"
msgstr "अनुप्रयोगाः"

#: ui/main.qml:188
#, kde-format
msgid "Login Scripts"
msgstr "प्रवेश लिपि"

#: ui/main.qml:191
#, kde-format
msgid "Pre-startup Scripts"
msgstr "पूर्व-प्रारम्भ स्क्रिप्ट्"

#: ui/main.qml:194
#, kde-format
msgid "Logout Scripts"
msgstr "लॉगआउट स्क्रिप्ट्"

#: ui/main.qml:203
#, kde-format
msgid "No user-specified autostart items"
msgstr "उपयोक्तृनिर्दिष्टानि स्वप्रारम्भवस्तूनि नास्ति"

#: ui/main.qml:204
#, kde-kuit-format
msgctxt "@info 'some' refers to autostart items"
msgid "Click the <interface>Add New</interface> button to add some"
msgstr "केचन योजयितुं <interface>Add New</interface> इति बटन् नुदन्तु"

#: ui/main.qml:219
#, kde-format
msgid "Choose Login Script"
msgstr "Login Script इति चिनोतु"

#: ui/main.qml:239
#, kde-format
msgid "Choose Logout Script"
msgstr "Logout Script इति चिनोतु"

#: unit.cpp:26
#, kde-format
msgctxt "@label Entry is running right now"
msgid "Running"
msgstr "चलति"

#: unit.cpp:27
#, kde-format
msgctxt "@label Entry is not running right now (exited without error)"
msgid "Not running"
msgstr "न धावति"

#: unit.cpp:28
#, kde-format
msgctxt "@label Entry is being started"
msgid "Starting"
msgstr "आरभ्यते"

#: unit.cpp:29
#, kde-format
msgctxt "@label Entry is being stopped"
msgid "Stopping"
msgstr "स्थगितम्"

#: unit.cpp:30
#, kde-format
msgctxt "@label Entry has failed (exited with an error)"
msgid "Failed"
msgstr "असफल"

#: unit.cpp:83
#, kde-format
msgid "Error occurred when receiving reply of GetAll call %1"
msgstr "GetAll call %1 इत्यस्य उत्तरं प्राप्य त्रुटिः अभवत्"

#: unit.cpp:155
#, kde-format
msgid "Failed to open journal"
msgstr "पत्रिकां उद्घाटयितुं असफलम्"
