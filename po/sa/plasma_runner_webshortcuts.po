#: webshortcutrunner.cpp:45 webshortcutrunner.cpp:137 webshortcutrunner.cpp:154
#, kde-format
msgid "Search %1 for %2"
msgstr "%2 इत्यस्य कृते %1 अन्वेष्टुम्"

#: webshortcutrunner.cpp:69
#, kde-format
msgid "Opens \"%1\" in a web browser with the query :q:."
msgstr ":q: इति प्रश्नेन सह जालपुटे \"%1\" उद्घाटयति ।"

#: webshortcutrunner.cpp:74
#, kde-format
msgid "Search using the DuckDuckGo bang syntax"
msgstr "DuckDuckGo bang वाक्यविन्यासस्य उपयोगेन अन्वेषणं कुर्वन्तु"

#: webshortcutrunner.cpp:102
#, kde-format
msgid "Search in private window"
msgstr "निजविण्डो मध्ये अन्वेषणं कुर्वन्तु"

#: webshortcutrunner.cpp:102
#, kde-format
msgid "Search in incognito window"
msgstr "गुप्तविण्डो मध्ये अन्वेषणं कुर्वन्तु"
