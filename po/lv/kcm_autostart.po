#: autostartmodel.cpp:376
#, kde-format
msgid "\"%1\" is not an absolute url."
msgstr "„%1“ ir absolūta URL adrese."

#: autostartmodel.cpp:379
#, kde-format
msgid "\"%1\" does not exist."
msgstr "„%1“ neeksistē."

#: autostartmodel.cpp:382
#, kde-format
msgid "\"%1\" is not a file."
msgstr "„%1“ nav datne."

#: autostartmodel.cpp:385
#, kde-format
msgid "\"%1\" is not readable."
msgstr "„%1“ nav nolasāms."

#: ui/entry.qml:52
#, kde-format
msgctxt ""
"@label The name of a Systemd unit for an app or script that will autostart"
msgid "Name:"
msgstr "Nosaukums:"

#: ui/entry.qml:58
#, kde-format
msgctxt ""
"@label The current status (e.g. active or inactive) of a Systemd unit for an "
"app or script that will autostart"
msgid "Status:"
msgstr "Stāvoklis:"

#: ui/entry.qml:64
#, kde-format
msgctxt ""
"@label A date and time follows this text, making a sentence like 'Last "
"activated on: August 7th 11 PM 2023'"
msgid "Last activated on:"
msgstr "Pēdējo reizi palaists:"

#: ui/entry.qml:74
#, kde-format
msgctxt "@label Stop the Systemd unit for a running process"
msgid "Stop"
msgstr "Apturēt"

#: ui/entry.qml:74
#, kde-format
msgctxt "@label Start the Systemd unit for a currently inactive process"
msgid "Start"
msgstr "Sākt"

#: ui/entry.qml:109
#, kde-format
msgid "Unable to load logs. Try refreshing."
msgstr "Neizdodas ielādēt žurnālus. Mēģiniet pārlādēt."

#: ui/entry.qml:113
#, kde-format
msgctxt "@action:button Refresh entry logs when it failed to load"
msgid "Refresh"
msgstr "Pārlādēt"

#: ui/main.qml:33
#, kde-format
msgid "Make Executable"
msgstr "Padarīt izpildāmu"

#: ui/main.qml:53
#, kde-format
msgid "The file '%1' must be executable to run at logout."
msgstr "Datnei „%1“ ir jābūt izpildāmai, lai to palaistu izrakstoties."

#: ui/main.qml:55
#, kde-format
msgid "The file '%1' must be executable to run at login."
msgstr "Datnei „%1“ ir jābūt izpildāmai, lai to palaistu ierakstoties."

#: ui/main.qml:66
#, kde-format
msgctxt "@action:button"
msgid "Add…"
msgstr "Pievienot..."

#: ui/main.qml:69
#, kde-format
msgctxt "@action:button"
msgid "Add Application…"
msgstr "Pievienot programmu..."

#: ui/main.qml:74
#, kde-format
msgctxt "@action:button"
msgid "Add Login Script…"
msgstr "Pievienot ierakstīšanās skriptu..."

#: ui/main.qml:79
#, kde-format
msgctxt "@action:button"
msgid "Add Logout Script…"
msgstr "Pievienot izrakstīšanās skriptu..."

#: ui/main.qml:114
#, kde-format
msgid ""
"%1 has not been autostarted yet. Details will be available after the system "
"is restarted."
msgstr ""
"„%1“ vēl nav automātiski palaists. Informācija būs pieejama pēc sistēmas "
"pārstartēšanas."

#: ui/main.qml:137
#, kde-format
msgctxt ""
"@label Entry hasn't been autostarted because system hasn't been restarted"
msgid "Not autostarted yet"
msgstr "Vēl nav automātiski palaists"

#: ui/main.qml:146
#, kde-format
msgctxt "@action:button"
msgid "See properties"
msgstr "Skatīt īpašības"

#: ui/main.qml:157
#, kde-format
msgctxt "@action:button"
msgid "Remove entry"
msgstr "Noņemt ierakstu"

#: ui/main.qml:173
#, kde-format
msgid "Applications"
msgstr "Programmas"

#: ui/main.qml:176
#, kde-format
msgid "Login Scripts"
msgstr "Ierakstīšanās skripti"

#: ui/main.qml:179
#, kde-format
msgid "Pre-startup Scripts"
msgstr "Pirmspalaišanas skripti"

#: ui/main.qml:182
#, kde-format
msgid "Logout Scripts"
msgstr "Izrakstīšanās skripti"

#: ui/main.qml:191
#, kde-format
msgid "No user-specified autostart items"
msgstr "Nav lietotāja iestatītu automātiskas palaišanās vienumu"

#: ui/main.qml:192
#, kde-kuit-format
msgctxt "@info 'some' refers to autostart items"
msgid "Click the <interface>Add…</interface> button to add some"
msgstr "Spiediet uz <interface>Pievienot...</interface>, lai kādu pievienotu"

#: ui/main.qml:207
#, kde-format
msgid "Choose Login Script"
msgstr "Izvēlieties ierakstīšanās skriptu"

#: ui/main.qml:227
#, kde-format
msgid "Choose Logout Script"
msgstr "Izvēlieties izrakstīšanās skriptu"

#: unit.cpp:26
#, kde-format
msgctxt "@label Entry is running right now"
msgid "Running"
msgstr "Darbojas"

#: unit.cpp:27
#, kde-format
msgctxt "@label Entry is not running right now (exited without error)"
msgid "Not running"
msgstr "Nav palaists"

#: unit.cpp:28
#, kde-format
msgctxt "@label Entry is being started"
msgid "Starting"
msgstr "Sāk darbību"

#: unit.cpp:29
#, kde-format
msgctxt "@label Entry is being stopped"
msgid "Stopping"
msgstr "Notiek apturēšana"

#: unit.cpp:30
#, kde-format
msgctxt "@label Entry has failed (exited with an error)"
msgid "Failed"
msgstr "Neizdevās"

#: unit.cpp:83
#, kde-format
msgid "Error occurred when receiving reply of GetAll call %1"
msgstr "Kļūda, saņemot atbildi no „GetAll“ izsauktās komandas %1"

#: unit.cpp:155
#, kde-format
msgid "Failed to open journal"
msgstr "Neizdevās atvērt žurnālu"

