msgstr[3] "%1 programi so zahtevali vklop načina varčevanja z energijo"

#: package/contents/ui/main.qml:200
#, kde-format
msgid "System is in Power Save mode"
msgstr "Sistem je v načinu varčevanja z energijo"

#: package/contents/ui/main.qml:260
#, kde-format
msgid "The battery applet has enabled system-wide inhibition"
msgstr ""
"Programček za baterijo je omogočil sistemsko preprečevanje varčevanja z "
"energijo"

#: package/contents/ui/main.qml:299
#, kde-format
msgid "Failed to activate %1 mode"
msgstr "Vklop načina %1 ni uspel"

