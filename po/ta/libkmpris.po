#: kded/mprisservice.cpp:131
#, kde-format
msgctxt "@title Name for global shortcuts category"
msgid "Media Controller"
msgstr "ஊடக கட்டுப்படுத்தி"

#: kded/mprisservice.cpp:140
#, kde-format
msgctxt "@title shortcut"
msgid "Play/Pause media playback"
msgstr "ஊடகத்தை இயக்கு/இடைநிறுத்து"

#: kded/mprisservice.cpp:141
#, kde-format
msgctxt "@title shortcut"
msgid "Media playback next"
msgstr "ஊடக இயக்கத்தில் அடுத்தது"

#: kded/mprisservice.cpp:142
#, kde-format
msgctxt "@title shortcut"
msgid "Media playback previous"
msgstr "ஊடக இயக்கத்தில் முந்தையது"

#: kded/mprisservice.cpp:144
#, kde-format
msgctxt "@title shortcut"
msgid "Media playback seek forward 5s"
msgstr "ஊடக இயக்கத்தில் 5 நொடி முன்னோக்கி செல்"

#: kded/mprisservice.cpp:148
#, kde-format
msgctxt "@title shortcut"
msgid "Media playback seek backward 5s"
msgstr "ஊடக இயக்கத்தில் 5 நொடி பின்னோக்கி செல்"

#: kded/mprisservice.cpp:152
#, kde-format
msgctxt "@title shortcut"
msgid "Media playback seek forward 30s"
msgstr "ஊடக இயக்கத்தில் 30 நொடி முன்னோக்கி செல்"

#: kded/mprisservice.cpp:156
#, kde-format
msgctxt "@title shortcut"
msgid "Media playback seek backward 30s"
msgstr "ஊடக இயக்கத்தில் 30 நொடி பின்னோக்கி செல்"

#: kded/mprisservice.cpp:159
#, kde-format
msgctxt "@title shortcut"
msgid "Stop media playback"
msgstr "ஊடக இயக்கத்தை நிறுத்து"

#: kded/mprisservice.cpp:160
#, kde-format
msgctxt "@title shortcut"
msgid "Pause media playback"
msgstr "ஊடக இயக்கத்தை இடைநிறுத்து"

#: kded/mprisservice.cpp:161
#, kde-format
msgctxt "@title shortcut"
msgid "Play media playback"
msgstr "ஊடக இயக்கத்தை இயக்கு"

#: kded/mprisservice.cpp:162
#, kde-format
msgctxt "@title shortcut"
msgid "Media volume up"
msgstr "ஊடக ஒலியளவை கூட்டு"

#: kded/mprisservice.cpp:163
#, kde-format
msgctxt "@title shortcut"
msgid "Media volume down"
msgstr "ஊடக ஒலியளவை குறை"

#~ msgctxt "@action:button"
#~ msgid "Choose player automatically"
#~ msgstr "இயக்கியை தானாக தேர்ந்தெடு"
