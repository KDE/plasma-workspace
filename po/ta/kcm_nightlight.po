msgctxt "Color temperature in Kelvin"
msgid "%1K"
msgstr "%1K"

#: ui/main.qml:204 ui/main.qml:266
#, kde-format
msgctxt "Night colour blue-ish; no blue light filter activated"
msgid "Cool (no filter)"
msgstr "நீலம் (நிறமாற்றம் இல்லாமல்)"

#: ui/main.qml:211 ui/main.qml:273
#, kde-format
msgctxt "Night colour red-ish"
msgid "Warm"
msgstr "சிவப்பு"

#: ui/main.qml:218
#, kde-format
msgid "Night light temperature:"
msgstr "இரவுக்கான நிறவெப்பநிலை:"

