msgid_plural "%1 minutes"
msgstr[0] "%1 நிமிடம்"
msgstr[1] "%1 நிமிடங்கள்"

#: ui/main.qml:407
#, kde-format
msgid "Input minutes - min. 1, max. 600"
msgstr "நிமிடங்களாக உள்ளிடவும் - குறைந்தது 1, அதிகபட்சம் 600"

#: ui/main.qml:426
#, kde-format
msgid "Error: Transition time overlaps."
msgstr "பிழை: நிலைமாற்ற நேரங்கள் முரண்படுகின்றன."

#: ui/main.qml:449
