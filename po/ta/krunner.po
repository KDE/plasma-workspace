#: main.cpp:57 view.cpp:49
#, kde-format
msgid "KRunner"
msgstr "கேரன்னர்"

#: main.cpp:57
#, kde-format
msgid "Run Command interface"
msgstr "கட்டளைகளை இயக்கும் இடைமுகப்பு"

#: main.cpp:63
#, kde-format
msgid "Use the clipboard contents as query for KRunner"
msgstr "பிடிப்புப்பலகையின் உள்ளடக்கத்தை கேரன்னரில் பயன்படுத்து"

#: main.cpp:64
#, kde-format
msgid "Start KRunner in the background, don't show it."
msgstr "கேரன்னரை பின்னணியில் தொடக்கி, அதைக் காட்டாதே."

#: main.cpp:65
#, kde-format
msgid "Replace an existing instance"
msgstr "ஏற்கனவே உள்ளதற்கு பதிலாக இது இயங்கட்டும்"

#: main.cpp:66
#, kde-format
msgid "Show only results from the given plugin"
msgstr "குறிப்பிட்ட செருகுநிரலிலிருந்தான முடிவுகளை மட்டும் காட்டு"

#: main.cpp:67
#, kde-format
msgid "List available plugins"
msgstr "உள்ள செருகுநிரல்களை பட்டியலிடு"

#: main.cpp:74
#, kde-format
msgid "The query to run, only used if -c is not provided"
msgstr "இயக்க வேண்டிய வினா, -c வழங்கப்படவில்லையெனில் மட்டும் பயன்படுத்தப்படும்"

#: main.cpp:83
#, kde-format
msgctxt "Header for command line output"
msgid "Available KRunner plugins, pluginId"
msgstr "உள்ள கேரன்னர் செருகுநிரல்கள், pluginId"

#: qml/RunCommand.qml:104
#, kde-format
