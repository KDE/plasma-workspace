#: package/contents/ui/DeviceItem.qml:188
#, kde-format
msgctxt "@info:status Free disk space"
msgid "%1 free of %2"
msgstr "%2-இல் %1 காலி"

#: package/contents/ui/DeviceItem.qml:192
#, kde-format
msgctxt ""
"Accessing is a less technical word for Mounting; translation should be short "
"and mean 'Currently mounting this device'"
msgid "Accessing…"
msgstr "அணுகப்படுகிறது…"

#: package/contents/ui/DeviceItem.qml:195
#, kde-format
msgctxt ""
"Removing is a less technical word for Unmounting; translation should be "
"short and mean 'Currently unmounting this device'"
msgid "Removing…"
msgstr "நீக்கப்படுகிறது…"

#: package/contents/ui/DeviceItem.qml:198
#, kde-format
msgid "Don't unplug yet! Files are still being transferred…"
msgstr "கழற்றாதீர்கள்! கோப்புகள் இன்னும் இடமாற்றப்படுகின்றன…"

#: package/contents/ui/DeviceItem.qml:229
#, kde-format
msgid "Open in File Manager"
msgstr "கோப்பு மேலாளரில் திற"

#: package/contents/ui/DeviceItem.qml:232
#, kde-format
msgid "Mount and Open"
msgstr "ஏற்றி திற"

#: package/contents/ui/DeviceItem.qml:234
#, kde-format
msgid "Eject"
msgstr "வெளியேற்று"

#: package/contents/ui/DeviceItem.qml:236
#, kde-format
msgid "Safely remove"
msgstr "பத்திரமாக நீக்கு"

#: package/contents/ui/DeviceItem.qml:278
#, kde-format
msgid "Mount"
msgstr "ஏற்று"

#: package/contents/ui/FullRepresentation.qml:43
#: package/contents/ui/main.qml:225
#, kde-format
msgid "Remove All"
msgstr "அனைத்தையும் நீக்கு"

#: package/contents/ui/FullRepresentation.qml:44
#, kde-format
msgid "Click to safely remove all devices"
msgstr "அனைத்து சாதனங்களையும் பத்திரமாக நீக்க க்ளிக் செய்யவும்"

#: package/contents/ui/FullRepresentation.qml:186
#, kde-format
msgid "No removable devices attached"
msgstr "கழற்றக்கூடிய சாதனங்கள் ஏதுமில்லை"

#: package/contents/ui/FullRepresentation.qml:186
#, kde-format
msgid "No disks available"
msgstr "வட்டுகள் ஏதுமில்லை"

#: package/contents/ui/main.qml:57
#, kde-format
msgid "Most Recent Device"
msgstr "மிக சமீபமான சாதனம்"

#: package/contents/ui/main.qml:57
#, kde-format
msgid "No Devices Available"
msgstr "எந்த சாதனமும் இல்லை"

#: package/contents/ui/main.qml:207
#, kde-format
msgctxt "Open auto mounter kcm"
msgid "Configure Removable Devices…"
msgstr "கழற்றக்கூடிய சாதனங்களை அமை…"

#: package/contents/ui/main.qml:233
#, kde-format
msgid "Removable Devices"
msgstr "கழற்றக்கூடிய சாதனங்கள்"

#: package/contents/ui/main.qml:248
#, kde-format
msgid "Non Removable Devices"
msgstr "கழற்ற முடியாத சாதனங்கள்"

#: package/contents/ui/main.qml:263
#, kde-format
msgid "All Devices"
msgstr "அனைத்து சாதனங்கள்"

#: package/contents/ui/main.qml:280
#, kde-format
msgid "Show popup when new device is plugged in"
msgstr "புதிய சாதனம் செருகப்படும்போது தெரித்தெழும் பட்டியை காட்டு"

#: plugin/ksolidnotify.cpp:192
#, kde-format
msgid "Device Status"
msgstr "சாதன நிலை"

#: plugin/ksolidnotify.cpp:193
#, kde-format
msgid "A device can now be safely removed"
msgstr "ஒரு சாதனத்தை இப்போது பத்திரமாக கழற்றலாம்"

#: plugin/ksolidnotify.cpp:197
#, kde-format
msgid "This device can now be safely removed."
msgstr "இந்த சாதனத்தை இப்போது பத்திரமாக கழற்றலாம்"

#: plugin/ksolidnotify.cpp:204
#, kde-format
msgid "You are not authorized to mount this device."
msgstr "இந்த சாதனத்தை ஏற்றும் உரிமை உங்களிடம் இல்லை."

#: plugin/ksolidnotify.cpp:207
#, kde-format
msgctxt "Remove is less technical for unmount"
msgid "You are not authorized to remove this device."
msgstr "இந்த சாதனத்தை நீக்கும் உரிமை உங்களிடம் இல்லை."

#: plugin/ksolidnotify.cpp:210
#, kde-format
msgid "You are not authorized to eject this disc."
msgstr "இந்த வட்டை வெளியேற்றும் உரிமை உங்களிடம் இல்லை."

#: plugin/ksolidnotify.cpp:217
#, kde-format
msgid "Could not mount this device as it is busy."
msgstr "இந்த சாதனம் ஏதோ வேலையாக உள்ளதால் ஏற்ற முடியவில்லை."

#: plugin/ksolidnotify.cpp:248
#, kde-format
msgid "One or more files on this device are open within an application."
msgstr "இந்த சாதனத்திலுள்ள ஒரு அல்லது பல கோப்புகள் ஏதோவொரு செயலியில் திறந்துள்ளன."

#: plugin/ksolidnotify.cpp:250
#, kde-format
msgid "One or more files on this device are opened in application \"%2\"."
msgid_plural ""
"One or more files on this device are opened in following applications: %2."
msgstr[0] ""
"இந்த சாதனத்தில் உள்ள ஒரு அல்லது பல கோப்புகள் \"%2\" என்ற செயலியில் திறந்துள்ளன."
msgstr[1] ""
"இந்த சாதனத்தில் உள்ள ஒரு அல்லது பல கோப்புகள் பின்வரும் செயலிகளில் திறந்துள்ளன: %2."

#: plugin/ksolidnotify.cpp:253
#, kde-format
msgctxt "separator in list of apps blocking device unmount"
msgid ", "
msgstr ", "

#: plugin/ksolidnotify.cpp:271
#, kde-format
msgid "Could not mount this device."
msgstr "இந்த சாதனத்தை ஏற்ற முடியவில்லை."

#: plugin/ksolidnotify.cpp:274
#, kde-format
msgctxt "Remove is less technical for unmount"
msgid "Could not remove this device."
msgstr "இந்த சாதனத்தை நீக்க முடியவில்லை."

#: plugin/ksolidnotify.cpp:277
#, kde-format
msgid "Could not eject this disc."
msgstr "இந்த வட்டை வெளியேற்ற முடியவில்லை."

