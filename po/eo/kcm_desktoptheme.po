#: kcm.cpp:110
#, kde-format
msgid "Unable to create a temporary file."
msgstr "Nekapabla krei dumtempan dosieron."

#: kcm.cpp:121
#, kde-format
msgid "Unable to download the theme: %1"
msgstr "Nekapabla elŝuti the etoson: %1"

#: kcm.cpp:146
#, kde-format
msgid "Theme installed successfully."
msgstr "Etoso instaliĝis sukcese."

#: kcm.cpp:149 kcm.cpp:155
#, kde-format
