#: module.cpp:68
#, kde-format
msgctxt "@title"
msgid "Memory Shortage Avoided"
msgstr "Predídenie nedostatku pamäte"

#: module.cpp:71
#, kde-kuit-format
msgctxt "@info"
msgid ""
"<command>%1</command> has been terminated by the Linux kernel because the "
"system is low on memory. Consider closing unused applications or browser "
"tabs."
msgstr ""
"<command>%1</command> bol ukončený jadrom systému Linux, pretože systém má "
"málo pamäte. Zvážte zatvorenie nepoužívaných aplikácií alebo kariet "
"prehliadača."

#: module.cpp:77
#, kde-kuit-format
msgctxt "@info"
msgid ""
"The background service <command>%1</command> has been terminated by the "
"Linux kernel because the system is low on memory. Consider closing unused "
"applications or browser tabs."
msgstr ""
"Služba na pozadí <command>%1</command> bola ukončená jadrom systému Linux, "
"pretože systém má málo pamäte. Zvážte zatvorenie nepoužívaných aplikácií "
"alebo kariet prehliadača."

#: module.cpp:85
#, kde-format
msgctxt "@action"
msgid "Restart Application"
msgstr "Reštartovať aplikáciu"
