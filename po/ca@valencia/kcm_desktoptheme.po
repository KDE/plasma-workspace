#: kcm.cpp:111
#, kde-format
msgid "Unable to create a temporary file."
msgstr "No s'ha pogut crear el fitxer temporal."

#: kcm.cpp:122
#, kde-format
msgid "Unable to download the theme: %1"
msgstr "No s'ha pogut baixar el tema: %1"

#: kcm.cpp:147
#, kde-format
msgid "Theme installed successfully."
msgstr "El tema s'ha instal·lat correctament."

#: kcm.cpp:150 kcm.cpp:156
#, kde-format
msgid "Theme installation failed."
msgstr "No s'ha pogut instal·lar el tema."

#: kcm.cpp:250
#, kde-format
msgid "Removing theme failed: %1"
