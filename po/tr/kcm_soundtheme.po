msgstr[0] "Şu temellidir: %2"
msgstr[1] "Şunlar temellidir: %2"

#: ui/main.qml:109
#, kde-format
msgctxt ""
"@label Precedes a list of buttons which can be clicked to preview the "
"theme's sounds. Keep it very short"
msgid "Preview:"
msgstr "Önizleme:"

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview sound \"%1\""
msgstr "‘%1’ sesini önizle"

#: ui/main.qml:150
#, kde-format
msgctxt "@info:tooltip"
msgid "Preview the demo sound for the theme \"%1\""
msgstr "‘%1’ temasının tanıtım sesini önizle"

#: ui/main.qml:185
#, kde-format
msgctxt "%1 is a sound theme name. %2 is a sound name"
msgid "The %1 theme doesn't include the sound '%2'"
msgstr "%1 teması, ‘%2’ sesini içermiyor"

#: ui/main.qml:190
#, kde-format
msgid "Failed to preview sound: %1"
msgstr "Ses önizlenemedi: %1"
