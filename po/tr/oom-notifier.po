#: module.cpp:68
#, kde-format
msgctxt "@title"
msgid "Memory Shortage Avoided"
msgstr "Bellek Sıkıntısı Önlendi"

#: module.cpp:71
#, kde-kuit-format
msgctxt "@info"
msgid ""
"<command>%1</command> has been terminated by the Linux kernel because the "
"system is low on memory. Consider closing unused applications or browser "
"tabs."
msgstr ""
"<command>%1</command>, sistemde bellek yetersiz olduğu için Linux çekirdeği "
"tarafından sonlandırıldı. Kullanılmayan uygulamaları veya tarayıcı "
"sekmelerini kapatmayı düşünün."

#: module.cpp:77
#, kde-kuit-format
msgctxt "@info"
msgid ""
"The background service <command>%1</command> has been terminated by the "
"Linux kernel because the system is low on memory. Consider closing unused "
"applications or browser tabs."
msgstr ""
"<command>%1</command> arka plan hizmeti, sistemde bellek yetersiz olduğu "
"için Linux çekirdeği tarafından sonlandırıldı. Kullanılmayan uygulamaları "
"veya tarayıcı sekmelerini kapatmayı düşünün."

#: module.cpp:85
#, kde-format
msgctxt "@action"
msgid "Restart Application"
msgstr "Uygulamayı Yeniden Başlat"
