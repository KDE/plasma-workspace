#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "Oturumu kapatma, '%1' tarafından iptal edildi"

#: main.cpp:118
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME ayarlanmamış!"

#: main.cpp:122 main.cpp:128
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME dizini (%1) yok."

#: main.cpp:124
#, kde-format
msgid "No write access to $HOME directory (%1)."
msgstr "$HOME dizinine (%1) yazma izni yok."

#: main.cpp:130
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "$HOME dizinini (%1) okuma izni yok."

