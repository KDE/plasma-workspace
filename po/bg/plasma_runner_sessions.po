#: sessionrunner.cpp:19
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to log out of the "
"session"
msgid "logout;log out"
msgstr "изход"

#: sessionrunner.cpp:22
#, kde-format
msgid "Logs out, exiting the current desktop session"
msgstr "Излизане от текущата сесия на работния плот"

#: sessionrunner.cpp:25
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to shut down the "
"computer"
msgid "shutdown;shut down;power;power off"
msgstr "изключване;спиране"

#: sessionrunner.cpp:28
#, kde-format
msgid "Turns off the computer"
msgstr "Изключване на компютъра"

#: sessionrunner.cpp:31
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to restart the "
"computer"
msgid "restart;reboot"
msgstr "рестартиране"

#: sessionrunner.cpp:34
#, kde-format
msgid "Reboots the computer"
msgstr "Рестартиране на компютъра"

#: sessionrunner.cpp:38
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to lock the screen"
msgid "lock;lock screen"
msgstr "заключване на екрана"

#: sessionrunner.cpp:40
#, kde-format
msgid "Locks the current sessions and starts the screen saver"
msgstr "Заключване на текущата сесия и включване на екранния предпазител"

#: sessionrunner.cpp:43
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to save the "
"desktop session"
msgid "save;save session"
msgstr "запазване на сесията"

#: sessionrunner.cpp:46
#, kde-format
msgid "Saves the current session for session restoration"
msgstr "Запазване на текущата сесия за последващо възстановяване"

#: sessionrunner.cpp:49
#, kde-format
msgctxt ""
"KRunner keywords (split by semicolons without whitespace) to switch user "
"sessions"
msgid "switch user;new session"
msgstr "превключване на потребител;нова сесия"

#: sessionrunner.cpp:52
#, kde-format
msgid "Switch to a session as a different user"
msgstr "Превключване към сесия като друг потребител"

#: sessionrunner.cpp:74
#, kde-format
msgctxt "log out command"
msgid "Log Out"
msgstr "Излизане от сесия"

#: sessionrunner.cpp:82
#, kde-format
msgctxt "turn off computer command"
msgid "Shut Down"
msgstr "Изключване"

#: sessionrunner.cpp:89
#, kde-format
msgctxt "restart computer command"
msgid "Restart"
msgstr "Рестартиране"

#: sessionrunner.cpp:96
#, kde-format
msgctxt "lock screen command"
msgid "Lock"
msgstr "Заключване"

#: sessionrunner.cpp:103
#, kde-format
msgid "Save Session"
msgstr "Запазване на сесията"

#: sessionrunner.cpp:110
#, kde-format
msgid "Switch User"
msgstr "Превключване на потребител"

#~ msgctxt "KRunner keyword to list user sessions"
#~ msgid "sessions"
#~ msgstr "сесии"

#~ msgid "Lists all sessions"
#~ msgstr "Списък на всички сесии"

#~ msgctxt "KRunner keyword to switch user sessions"
#~ msgid "switch"
#~ msgstr "превключване"

#~ msgid ""
#~ "Switches to the active session for the user :q:, or lists all active "
#~ "sessions if :q: is not provided"
#~ msgstr ""
#~ "Превключване към активната сесия на потребителя :q: или показване на "
#~ "списък с всички активни сесии, ако не е зададен :q:"

#~ msgid ""
#~ "<p>You are about to enter a new desktop session.</p><p>A login screen "
#~ "will be displayed and the current session will be hidden.</p><p>You can "
#~ "switch between desktop sessions using:</p><ul><li>Ctrl+Alt+F{number of "
#~ "session}</li><li>Plasma search (type '%1')</li><li>Plasma widgets (such "
#~ "as the application launcher)</li></ul>"
#~ msgstr ""
#~ "<p>Ще влезете в нова сесия на работния плот.</p><p>Ще се появи екран за "
#~ "вход и текущата сесия ще бъде скрита.</p><p>Можете да превключвате между "
#~ "сесиите на работния плот, като използвате:</p><ul><li>Ctrl+Alt+F{номер на "
#~ "сесия}</li><li>Търсене на Plasma (въведете „%1“)</li><li>Уиджети на "
#~ "Plasma (като например програмата за стартиране на приложения)</li></ul>"

#~ msgid "New Desktop Session"
#~ msgstr "Нова сесия на работната среда"
