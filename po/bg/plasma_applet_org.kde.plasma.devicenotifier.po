#: plugin/actions/mountandopenaction.cpp:166
#, kde-format
msgid "Open in File Manager"
msgstr "Отваряне във Файлов мениджър"

#: plugin/actions/mountandopenaction.cpp:169
#, kde-format
msgid "Mount and Open"
msgstr "Монтиране и отваряне"

#: plugin/actions/mountandopenaction.cpp:171
#, kde-format
msgid "Eject"
msgstr "Изваждане"

#: plugin/actions/mountandopenaction.cpp:173
#, kde-format
msgid "Safely remove"
msgstr "Безопасно отстраняване"

#: plugin/deviceerrormonitor_p.cpp:180
#, kde-format
