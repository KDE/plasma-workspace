#: package/contents/ui/DeviceItem.qml:232
#, kde-format
msgid "Mount and Open"
msgstr "Монтиране и отваряне"

#: package/contents/ui/DeviceItem.qml:234
#, kde-format
msgid "Eject"
msgstr "Изваждане"

#: package/contents/ui/DeviceItem.qml:236
#, kde-format
