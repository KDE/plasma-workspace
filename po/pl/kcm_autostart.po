#, kde-format
msgctxt "@action:button Refresh entry logs when it failed to load"
msgid "Refresh"
msgstr "Odśwież"

#: ui/main.qml:33
#, kde-format
msgid "Make Executable"
msgstr "Uczyń wykonywalnym"

#: ui/main.qml:53
#, kde-format
msgid "The file '%1' must be executable to run at logout."
msgstr ""
"Plik '%1' musi być wykonywalnym, aby móc go uruchomi przy wylogowywaniu."

#: ui/main.qml:55
#, kde-format
msgid "The file '%1' must be executable to run at login."
msgstr "Plik '%1' musi być wykonywalnym, aby móc go uruchomi przy logowaniu."

