#: module.cpp:68
#, kde-format
msgctxt "@title"
msgid "Memory Shortage Avoided"
msgstr "Komið í veg fyrir skort á minni"

#: module.cpp:71
#, kde-kuit-format
msgctxt "@info"
msgid ""
"<command>%1</command> has been terminated by the Linux kernel because the "
"system is low on memory. Consider closing unused applications or browser "
"tabs."
msgstr ""
"Linux-kjarninn slökkti á <command>%1</command> þar sem lítið minni var eftir "
"á kerfinu. Íhugaðu að loka ónotuðum forritum og flipum í vöfrum."

#: module.cpp:77
#, kde-kuit-format
msgctxt "@info"
msgid ""
"The background service <command>%1</command> has been terminated by the "
"Linux kernel because the system is low on memory. Consider closing unused "
"applications or browser tabs."
msgstr ""
"Linux-kjarninn slökkti á <command>%1</command> bakgrunnsþjónustunni þar sem "
"lítið minni var eftir á kerfinu. Íhugaðu að loka ónotuðum forritum og flipum "
"í vöfrum."

#: module.cpp:85
#, kde-format
msgctxt "@action"
msgid "Restart Application"
msgstr "Endurræsa forrit"
