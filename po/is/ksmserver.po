#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "Hætt við útskráningu af '%1'"

#: main.cpp:77
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME ekki stillt!"

#: main.cpp:81 main.cpp:87
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME-mappan (%1) er ekki til."

#: main.cpp:83
#, kde-format
msgid "No write access to $HOME directory (%1)."
msgstr "Enginn skrifaðgangur að $HOME-möppunni (%1)."

#: main.cpp:89
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "Enginn lesaðgangur að $HOME-möppunni (%1)."

#: main.cpp:93
#, kde-format
msgid "$HOME directory (%1) is out of disk space."
msgstr "HOME-möppu (%1) vantar diskpláss."

#: main.cpp:96
#, kde-format
msgid "Writing to the $HOME directory (%2) failed with the error '%1'"
msgstr "Mistókst að skrifa í $HOME-möppuna (%2) með villunni '%1'"

#: main.cpp:109 main.cpp:144
#, kde-format
msgid "No write access to '%1'."
msgstr "Enginn skrifaðgangur að '%1'."

#: main.cpp:111 main.cpp:146
#, kde-format
msgid "No read access to '%1'."
msgstr "Enginn lesaðgangur að '%1'."

#: main.cpp:119 main.cpp:132
#, kde-format
msgid "Temp directory (%1) is out of disk space."
msgstr "Bráðabigðamöppu (%1) vantar diskpláss."

#: main.cpp:122 main.cpp:135
#, kde-format
msgid ""
"Writing to the temp directory (%2) failed with\n"
"    the error '%1'"
msgstr ""
"Mistókst að skrifa í bráðabirðgamöppuna (%2) með\n"
"    villunni'%1'"

#: main.cpp:150
#, kde-format
msgid ""
"The following installation problem was detected\n"
"while trying to start Plasma:"
msgstr ""
"Eftirfarandi uppsetningarvandamál fundust\n"
"þegar reynt var að keyra Plasma:"

#: main.cpp:153
#, kde-format
msgid ""
"\n"
"\n"
"Plasma is unable to start.\n"
msgstr ""
"\n"
"\n"
"Tekst ekki að ræsa Plasma.\n"

#: main.cpp:160
#, kde-format
msgid "Plasma Workspace installation problem!"
msgstr "Vandamál við uppsetningu Plasma-vinnusvæðis!"

#: main.cpp:194
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"Áreiðanlegi Plasma setustjórinn sem skilur almenna X11R6 \n"
"setustjórnunarstaðalinn (e. session management protocol, XSMP)."

#: main.cpp:198
#, kde-format
msgid "Restores the saved user session if available"
msgstr "Endurheimtir síðustu setu notanda, ef hægt er"

#: main.cpp:201
#, kde-format
msgid "Also allow remote connections"
msgstr "Leyfa einnig utanaðkomandi tengingar"

#: main.cpp:204
#, kde-format
msgid "Starts the session in locked mode"
msgstr "Byrja setuna í læstri stillingu"

#: main.cpp:208
#, kde-format
msgid ""
"Starts without lock screen support. Only needed if other component provides "
"the lock screen."
msgstr ""
"Byrjar án stuðnings við lásskjá. Einungis er þörf á þessu ef aðrir hlutar "
"útvega lásskjáinn."

#: server.cpp:884
#, kde-format
msgid "Session Management"
msgstr "Setustýring"

#: server.cpp:887
#, kde-format
msgid "Log Out"
msgstr "Skrá út"

#: server.cpp:892
#, kde-format
msgid "Log Out Without Confirmation"
msgstr "Skrá út án staðfestingar"

#: server.cpp:897
#, kde-format
msgid "Halt Without Confirmation"
msgstr "Stöðva tölvu án staðfestingar"

#: server.cpp:902
#, kde-format
msgid "Reboot Without Confirmation"
msgstr "Endurræsa tölvu án staðfestingar"

