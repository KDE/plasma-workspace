#, kde-format
msgctxt "Color temperature in Kelvin"
msgid "%1K"
msgstr "%1K"

#: ui/main.qml:222 ui/main.qml:281
#, kde-format
msgctxt "Night colour red-ish"
msgid "Warm"
msgstr "თბილი"

#: ui/main.qml:228 ui/main.qml:287
#, kde-format
msgctxt "No blue light filter activated"
msgid "Cool (no filter)"
msgstr "მაგარი (ფილტრის გარეშე)"

#: ui/main.qml:228 ui/main.qml:287
#, kde-format
msgctxt "Night colour blue-ish"
msgid "Cool"
msgstr "გრილი"

#: ui/main.qml:234
#, kde-format
msgid "Night color temperature:"
msgstr "ღამის ფერების ტემპერატურა:"

#: ui/main.qml:298
#, kde-format
msgid "Latitude: %1°   Longitude: %2°"
msgstr "განედი: %1°  გრძედი: %2°"

#: ui/main.qml:308
#, kde-format
msgid "Begin night color at:"
msgstr "ღამის ფერების დაწყება:"

#: ui/main.qml:321 ui/main.qml:344
#, kde-format
msgid "Input format: HH:MM"
msgstr "შეიყვანეთ დრო: სს:წწ"

#: ui/main.qml:331
#, kde-format
msgid "Begin day color at:"
msgstr "დღის ფერების დაწყება:"

#: ui/main.qml:353
#, kde-format
msgid "Transition duration:"
msgstr "გადასვლის დრო:"

#: ui/main.qml:362
#, kde-format
msgid "%1 minute"
msgid_plural "%1 minutes"
msgstr[0] "%1 წუთი"
msgstr[1] "%1 წუთი"

#: ui/main.qml:375
#, kde-format
msgid "Input minutes - min. 1, max. 600"
msgstr "შეიყვანეთ წუთები - მინ 1, მაქს 600"

#: ui/main.qml:393
#, kde-format
msgid "Error: Transition time overlaps."
msgstr "შეცდომა: გადასვლის დროის გადაფარვა."

#: ui/main.qml:413
#, kde-format
msgctxt "@info:placeholder"
msgid "Locating…"
msgstr "მდებარეობა…"

#: ui/TimingsView.qml:32
#, kde-format
msgid ""
"Color temperature begins changing to night time at %1 and is fully changed "
"by %2"
msgstr ""
"ფერის ტემპერატურა ღამის ფერებზე გადასვლას %1-ზე დაიწყებს და სრულად %2-ზე "
"გადავა"

#: ui/TimingsView.qml:39
#, kde-format
msgid ""
"Color temperature begins changing to day time at %1 and is fully changed by "
"%2"
msgstr ""
"ფერის ტემპერატურა დღის ფერებზე გადასვლას %1-ზე დაიწყებს და სრულად %2-ზე "
"გადავა"

