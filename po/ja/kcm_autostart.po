#, kde-format
msgid "\"%1\" does not exist."
msgstr "\"%1\" は存在しません。"

#: autostartmodel.cpp:382
#, kde-format
msgid "\"%1\" is not a file."
msgstr "\"%1\" はファイルではありません。"

