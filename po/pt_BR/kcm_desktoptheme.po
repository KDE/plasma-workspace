#, kde-format
msgid "Name of the current Plasma Style"
msgstr "Nome do estilo do Plasma atual"

#: kcm.cpp:111
#, kde-format
msgid "Unable to create a temporary file."
msgstr "Não foi possível criar um arquivo temporário."

#: kcm.cpp:122
#, kde-format
msgid "Unable to download the theme: %1"
msgstr "Não foi possível baixar o tema: %1"

#: kcm.cpp:147
#, kde-format
msgid "Theme installed successfully."
msgstr "Tema instalado com sucesso."

#: kcm.cpp:150 kcm.cpp:156
