#, kde-format
msgctxt "@action:button"
msgid "See properties"
msgstr "Vedi le proprietà"

#: ui/main.qml:157
#, kde-format
msgctxt "@action:button"
msgid "Remove entry"
msgstr "Rimuovi voce"

