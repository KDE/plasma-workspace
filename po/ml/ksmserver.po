#: logout.cpp:340
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "പുറത്തിറങ്ങുന്നതു് %1 റദ്ദാക്കി"

#: main.cpp:118
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME സജ്ജമാക്കിയിട്ടില്ല!"

#: main.cpp:122 main.cpp:128
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "$HOME ഹോം കൂട (%1) നിലവിലില്ല."

#: main.cpp:124
#, kde-format
msgid "No write access to $HOME directory (%1)."
msgstr "$HOME കൂടയിലേക്ക് എഴുത്തവകാശമില്ല (%1)."

#: main.cpp:130
#, kde-format
msgid "No read access to $HOME directory (%1)."
msgstr "$HOME കൂടയിലേക്ക് വായനാവകാശമില്ല (%1)."

#: main.cpp:134
#, kde-format
msgid "$HOME directory (%1) is out of disk space."
msgstr "$HOME കൂടയിൽ (%1) ഡിസ്ക് ഇടമില്ല."

#: main.cpp:137
#, kde-format
msgid "Writing to the $HOME directory (%2) failed with the error '%1'"
msgstr "$HOME കൂടയിലേക്ക് (%2) എഴുതുന്നത് '%1' പിശക് കാരണം പരാജയപ്പെട്ടു"

#: main.cpp:150 main.cpp:185
#, kde-format
msgid "No write access to '%1'."
msgstr "'%1' ലേക്ക് എഴുത്തവകാശമില്ല."

#: main.cpp:152 main.cpp:187
#, kde-format
msgid "No read access to '%1'."
msgstr "'%1' ലേക്ക് വായനാവകാശമില്ല."

#: main.cpp:160 main.cpp:173
#, kde-format
msgid "Temp directory (%1) is out of disk space."
msgstr "താൽക്കാലിക കൂടയിൽ (%1) ഡിസ്ക് ഇടമില്ല."

#: main.cpp:163 main.cpp:176
#, kde-format
msgid ""
"Writing to the temp directory (%2) failed with\n"
"    the error '%1'"
msgstr ""
"താൽക്കാലിക കൂടയിലേക്ക് (%2) എഴുതുന്നത് \n"
"     %1 പിശക് കാരണം  പരാജയപ്പെട്ടു"

#: main.cpp:191
#, kde-format
msgid ""
"The following installation problem was detected\n"
"while trying to start Plasma:"
msgstr ""
"പ്ലാസ്മ ആരംഭിക്കാൻ ശ്രമിക്കുമ്പോൾ\n"
"ഇനിപ്പറയുന്ന ഇൻസ്റ്റാളേഷൻ പിശക് കണ്ടെത്തി:"

#: main.cpp:194
#, kde-format
msgid ""
"\n"
"\n"
"Plasma is unable to start.\n"
msgstr ""
"\n"
"\n"
"പ്ലാസ്മ ആരംഭിക്കാൻ കഴിഞ്ഞില്ല.\n"

#: main.cpp:201
#, kde-format
msgid "Plasma Workspace installation problem!"
msgstr "പ്ലാസ്മ പണിയിടം ഇൻസ്റ്റാളേഷൻ പ്രശ്നം!"

#: main.cpp:237
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"സ്റ്റാൻഡേർഡ് എക്സ് 11 ആർ 6 മായി സംവദിക്കുന്ന വിശ്വസനീയമായ പ്ലാസ്മ സെഷൻ മാനേജർ\n"
"സെഷൻ മാനേജുമെന്റ് പ്രോട്ടോക്കോൾ (എക്സ്എസ്എംപി)."

#: main.cpp:241
#, kde-format
msgid "Restores the saved user session if available"
msgstr "ഉപയോക്താവിന്റെ സൂക്ഷിച്ചിരിക്കുന്ന സെഷന്‍ തിരികെ കൊണ്ടുവരുന്നു ലഭ്യമാണെങ്കില്‍"

#: main.cpp:244
#, kde-format
msgid "Also allow remote connections"
msgstr "വിദൂര കണക്ഷനുകളും അനുവദിക്കുന്നു"

#: main.cpp:247
#, kde-format
msgid "Starts the session in locked mode"
msgstr "ലോക്കുചെയ്ത മോഡിൽ സെഷൻ ആരംഭിക്കുന്നു"

#: main.cpp:251
#, kde-format
msgid ""
"Starts without lock screen support. Only needed if other component provides "
"the lock screen."
msgstr ""
"ലോക്ക് സ്ക്രീൻ പിന്തുണയില്ലാതെ ആരംഭിക്കുന്നു. മറ്റ് ഘടകങ്ങൾ ലോക്ക് സ്ക്രീൻ നൽകുന്നുവെങ്കിൽ മാത്രം "
"ആവശ്യമാണ്."

#: server.cpp:853
#, kde-format
msgid "Session Management"
msgstr "സെഷന്‍ നടത്തിപ്പുകാരന്‍"

#: server.cpp:856
#, kde-format
msgid "Log Out"
msgstr "പുറത്തിറങ്ങുക"

#: server.cpp:861
#, kde-format
msgid "Log Out Without Confirmation"
msgstr "സ്ഥിരീകരണമില്ലാതെ പുറത്ത് കടക്കുക"

#: server.cpp:866
#, kde-format
msgid "Halt Without Confirmation"
msgstr "സ്ഥിരീകരണമില്ലാതെ നിർത്തുക"

#: server.cpp:871
#, kde-format
msgid "Reboot Without Confirmation"
msgstr "സ്ഥിരീകരണമില്ലാതെ പുനഃരാരംഭിക്കുക"

